{
  "name": "template-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "node swEnvbuild.js dev && msw init public/ --save && yarn server:dev ",
    "build": "rm -rf dist && node swEnvbuild.js prod && vite build --mode prod && yarn build:server",
    "build:dev": "rm -rf dist && node swEnvbuild.js dev && tsc && vite build --mode dev",
    "build:hml": "rm -rf dist && node swEnvbuild.js hml && tsc && vite build --mode hml",
    "build:server": "rm -rf distServer && vite build --config vite.config.node.ts",
    "server": "node --loader ts-node/esm ./server/index.ts",
    "server:dev": "NODE_ENV=dev yarn server",
    "server:hml": "NODE_ENV=hml yarn server",
    "server:prod": "NODE_ENV=prod yarn server",
    "preview": "yarn build && yarn server:prod",
    "preview:hml": "yarn build && yarn server:hml",
    "lint": "tsc && eslint --fix",
    "test": "vitest run --environment happy-dom",
    "test:coverage": "vitest run --coverage --environment happy-dom",
    "precommit": "yarn lint",
    "pretty": "prettier --config ./.prettierrc.json --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "deploy": "yarn build && npx firebase deploy --only hosting",
    "generate:component": "npx hygen generator component",
    "generate:scene": "npx hygen generator scene",
    "prepare": "husky install"
  },
  "resolutions": {
    "@types/react": "18",
    "@types/react-dom": "18",
    "@brillout/json-serializer": "0.5.8"
